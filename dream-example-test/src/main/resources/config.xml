<configuration xmlns="http://moxa.org/schema/config">
    <aliasFactory type="com.moxa.dream.driver.alias.DefaultAliasFactory">
        <property name="ds" value="com.moxa.dream.example.driver.MyDataSourceFactory"/>
    </aliasFactory>
    <cacheFactory type="com.moxa.dream.module.cache.factory.DefaultCacheFactory">
                <property name="enabled" value="false"/>
    </cacheFactory>
    <mapperFactory>
        <mapping package="com.moxa.dream.example.driver.mapper"/>
    </mapperFactory>
    <tableFactory>
        <mapping package="com.moxa.dream.example.driver.table"/>
    </tableFactory>
    <typeHandlerFactory type="com.moxa.dream.module.typehandler.factory.DefaultTypeHandlerFactory">
    </typeHandlerFactory>
    <pluginFactory type="com.moxa.dream.module.plugin.factory.JavaPluginFactory">
        <interceptor type="com.moxa.dream.driver.page.interceptor.PageInterceptor"/>
    </pluginFactory>
    <dialectFactory type="com.moxa.dream.driver.factory.DefaultDialectFactory" dialect="${mysql}">
    </dialectFactory>
    <transactionFactory type="com.moxa.dream.module.transaction.factory.JdbcTransactionFactory">
    </transactionFactory>
    <dataSourceFactory type="${ds}">
    </dataSourceFactory>
</configuration>